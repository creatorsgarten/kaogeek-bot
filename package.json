    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "dotenv": "16.0.3",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.8.8",
    "rimraf": "5.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": "^16.13 || ^18.12",
    "pnpm": ">=8"
